{
  "$schema": "http://json-schema.org/draft-07/schema#",

  "type": "object",

  "properties": {
    "age": { "type": "number" },
    "name": { "type": "string" },
    "gender": { "enum": [ "male", "female" ] },
    "company": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "phone": { "type": "string" },
    "address": { "type": "string"}
  },

  "required": ["name"],
  "additionalProperties": false
}
